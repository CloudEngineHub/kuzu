-DATASET CSV empty

--

-CASE SingleWCC
-STATEMENT CREATE NODE TABLE Node(id INT64 PRIMARY KEY);
---- ok
-STATEMENT CREATE REL TABLE Edge(FROM Node to Node);
---- ok
-STATEMENT CREATE (u0:Node {id: 0}),
            (u1:Node {id: 1}),
            (u2:Node {id: 2}),
            (u3:Node {id: 3}),
            (u4:Node {id: 4}),
            (u5:Node {id: 5}),
            (u6:Node {id: 6}),
            (u7:Node {id: 7}),
            (u8:Node {id: 8}),
            (u0)-[:Edge]->(u1),
            (u1)-[:Edge]->(u2),
            (u2)-[:Edge]->(u0),
            (u2)-[:Edge]->(u3),
            (u2)-[:Edge]->(u4);
---- ok
-STATEMENT CALL create_projected_graph('Graph', ['Node'], ['Edge']);
---- ok
-STATEMENT CALL gds_wcc('Graph')
    WITH group_id, min(node.id) as wccId, count(*) as nodeCount, list_sort(collect(node.id)) as nodeIds
    RETURN wccId, nodeCount, nodeIds ORDER BY wccId;
---- 5
0|5|[0,1,2,3,4]
5|1|[5]
6|1|[6]
7|1|[7]
8|1|[8]

